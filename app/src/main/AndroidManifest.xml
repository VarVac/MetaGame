<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.haoku.metagame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProfileListActivity" />
        <activity android:name=".NewProfileActivity" />
        <activity android:name=".ProfileViewActivity" />
        <activity android:name=".MatchListActivity" />
        <activity android:name=".NewMatchActivity" />
        <activity android:name=".ArchetypeListActivity" />
        <activity android:name=".NewArchetypeActivity" />
        <activity android:name=".MatchViewActivity" />
        <activity android:name=".FilterMatchesActivity" />
        <activity android:name=".ArchetypeViewActivity"></activity>
    </application>

</manifest>